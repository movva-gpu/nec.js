{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "cSpell.words": [
    "nonconstructor",
    "Parens",
    "tseslint"
  ],
  "html.customData": [
    "html.json"
  ],
  "conventionalCommits.scopes": [
    "all"
  ]
}
